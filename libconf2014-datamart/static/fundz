<script>
  function resetReturns () {
    jq('.bsmSelect option[value="budget-_-budget"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="bib-_-title"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="budget-_-type"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="budget-_-open_date"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="budget-_-local_sum"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="budget-_-credit_debit"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="budget-_-paid"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="orders-_-order_number"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="budget-_-invoice_no"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="orders-_-material_type"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    jq('.bsmSelect option[value="orders-_-method_of_acquisition"]:not(:selected)').attr('selected', 'selected');
    jq('.bsmSelect').trigger('change');
    }
  jq(document).ready(function() {
    resetReturns();
  });
</script>

